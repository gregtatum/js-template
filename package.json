{
  "name": "js-template",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "./bin/build.sh",
    "deploy": "bin/deploy.sh",
    "check-updates": "npx npm-check-updates",
    "start-ts": "tsc --noEmit --watch",
    "start-webpack": "NODE_ENV=development ./bin/server.js",
    "start": "run-p start-ts start-webpack",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet",
    "ts": "tsc --noEmit",
    "test": "env NODE_ENV=test jest",
    "test-all": "run-s lint ts test"
  },
  "alias": {
    "src": "./src"
  },
  "author": "Greg Tatum",
  "license": "MIT",
  "browserslist": [
    "since 2019-01"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!**/node_modules/**"
    ],
    "testEnvironment": "jsdom",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/src/test/__mocks__/style.ts"
    },
    "roots": [
      "src/test"
    ],
    "setupFiles": [
      "./src/test/setupFiles.ts"
    ],
    "setupFilesAfterEnv": [
      "./src/test/setupAfterEnv.ts"
    ],
    "globalTeardown": "./src/test/teardown.ts",
    "verbose": true
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.0",
    "@testing-library/react": "^12.1.5",
    "@types/jest": "^28.1.8",
    "@types/node-fetch": "^2.6.2",
    "@types/react": "^17.0.53",
    "@types/react-dom": "^17.0.19",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "babel-jest": "^28.1.3",
    "babel-loader": "^8.3.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.7.3",
    "dotenv": "^16.0.3",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "fetch-mock-jest": "^1.5.1",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "node-fetch": "^2.6.9",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "style-loader": "^3.3.2",
    "typescript": "^4.9.5",
    "webpack": "^5.76.1",
    "webpack-bundle-analyzer": "^4.8.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1",
    "workbox-webpack-plugin": "^6.5.4"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.9",
    "redux": "^4.2.1",
    "redux-thunk": "^2.4.2"
  }
}
